{
  "name": "@trex/cli",
  "version": "0.5.0",
  "description": "TREX server launcher",
  "bin": {
    "trex": "./bin/trex",
    "tbuild": "./bin/tbuild"
  },
  "files": [
    "bin/trex",
    "bin/tbuild",
    "bin/build.sh",
    "lib/"
  ],
  "scripts": {
    "build": "./bin/build.sh",
    "postinstall": "./bin/build.sh",
    "prepack": "npm run build",
    "start": "mkdir -p /usr/src/cert && echo \"$TLS__INTERNAL__CRT\" > /usr/src/cert/cert.pem && echo \"$TLS__INTERNAL__KEY\" > /usr/src/cert/key.pem && trex serve --tls-cert /usr/src/cert/cert.pem --tls-key /usr/src/cert/key.pem --tls-port 33000 --trexas-port 33001 --pgwire-port 5432 --main-path /usr/src/core/server --event-worker-path /usr/src/core/event",
    "startdev": "mkdir -p /usr/src/cert && echo \"$TLS__INTERNAL__CRT\" > /usr/src/cert/cert.pem && echo \"$TLS__INTERNAL__KEY\" > /usr/src/cert/key.pem && trex serve --tls-cert /usr/src/cert/cert.pem --tls-key /usr/src/cert/key.pem --tls-port 33000 --trexas-port 33001 --pgwire-port 5432 --main-path /usr/src/core/server --event-worker-path /usr/src/core/event --enable-inspector --inspector-host 0.0.0.0 --inspector-port 9229",
    "starttest": "mkdir -p /usr/src/cert && echo \"$TLS__INTERNAL__CRT\" > /usr/src/cert/cert.pem && echo \"$TLS__INTERNAL__KEY\" > /usr/src/cert/key.pem && trex serve --tls-cert /usr/src/cert/cert.pem --tls-key /usr/src/cert/key.pem --tls-port 33000 --trexas-port 33001 --pgwire-port 5432 --main-path /usr/src/main --event-worker-path /usr/src/event-worker",
    "health": "curl -f http://localhost:33001/_internal/health || exit 1"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@trex/as": "^0.5.0-1761961659-fb8964b5010b36c17b4462ce4da13b143dc7dcaa",
    "@trex/circe": "^0.0.1-1761998601-4cd8b8f9084286a87944460ed2ea434a12095cb1",
    "@trex/hana": "^0.5.0-1761998602-4cd8b8f9084286a87944460ed2ea434a12095cb1",
    "@trex/pgwire": "^0.5.0-1761998604-4cd8b8f9084286a87944460ed2ea434a12095cb1",
    "@trex/tpm": "^0.5.0-1761998603-4cd8b8f9084286a87944460ed2ea434a12095cb1"
  }
}
