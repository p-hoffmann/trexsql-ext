;; JDBC to DuckDB Type Mappings
;; Maps java.sql.Types constants to DuckDB type names
;; Used by trexsql.jdbc-types for schema generation during JDBC batch cache creation
;;
;; Format: {jdbc-type-int "DUCKDB_TYPE"}
;; See: java.sql.Types for constant values

{;; Numeric types
 -5   "BIGINT"      ; Types/BIGINT
 4    "INTEGER"     ; Types/INTEGER
 5    "SMALLINT"    ; Types/SMALLINT
 -6   "TINYINT"     ; Types/TINYINT
 3    "DECIMAL"     ; Types/DECIMAL
 2    "DECIMAL"     ; Types/NUMERIC
 8    "DOUBLE"      ; Types/DOUBLE
 6    "FLOAT"       ; Types/FLOAT
 7    "REAL"        ; Types/REAL

 ;; String types
 12   "VARCHAR"     ; Types/VARCHAR
 1    "VARCHAR"     ; Types/CHAR
 -9   "VARCHAR"     ; Types/NVARCHAR
 -15  "VARCHAR"     ; Types/NCHAR
 -1   "VARCHAR"     ; Types/LONGVARCHAR
 2005 "VARCHAR"     ; Types/CLOB
 2011 "VARCHAR"     ; Types/NCLOB
 -16  "VARCHAR"     ; Types/LONGNVARCHAR

 ;; Date/time types
 91   "DATE"        ; Types/DATE
 92   "TIME"        ; Types/TIME
 93   "TIMESTAMP"   ; Types/TIMESTAMP
 2014 "TIMESTAMP"   ; Types/TIMESTAMP_WITH_TIMEZONE

 ;; Boolean
 16   "BOOLEAN"     ; Types/BOOLEAN
 -7   "BOOLEAN"     ; Types/BIT

 ;; Binary types
 2004 "BLOB"        ; Types/BLOB
 -2   "BLOB"        ; Types/BINARY
 -3   "BLOB"        ; Types/VARBINARY
 -4   "BLOB"        ; Types/LONGVARBINARY

 ;; Other
 0    "VARCHAR"     ; Types/NULL
 1111 "VARCHAR"     ; Types/OTHER
 2000 "VARCHAR"     ; Types/JAVA_OBJECT
 2002 "VARCHAR"     ; Types/STRUCT
 2003 "VARCHAR"}    ; Types/ARRAY
